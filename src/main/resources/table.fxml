<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LeaderBoardController">
    <children>
        <TableView fx:id="tableView">
            <columns>
                <TableColumn fx:id="name" prefWidth="75.0" text="name"/>
                <TableColumn fx:id="color" prefWidth="75.0" text="color"/>
                <TableColumn fx:id="moves" prefWidth="75.0" text="moves"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</StackPane>
